{
  "private": true,
  "name": "example-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace client start",
    "graphql": "yarn workspace graphql-layer start",
    "start": "concurrently --kill-others-on-fail \"yarn graphql\"  \"yarn client\""
  },
  "dependencies": {
    "concurrently": "^4.1.2"
  }
}
